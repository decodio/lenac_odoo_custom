<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="0">
        <!-- OWNER EQUIPMENT PROJECT-->
        <record model="ir.ui.view" id="owner_equipment_project_search_view">
            <field name="name">owner.equipment.project.search</field>
            <field name="model">owner.equipment.project</field>
            <field name="arch" type="xml">
                <search string="Project">
                    <field name="name"/>
                    <field name="project_code_crm" string="Project Name CRM"/>
                    <field name="project_location_ids" string="Project Locations"/>
                    <field name="project_category_ids" string="Project Categories"/>
                </search>
            </field>
        </record>

        <record model="ir.ui.view" id="owner_equipment_project_view_tree" >
            <field name="name">owner.equipment.project.tree</field>
            <field name="model">owner.equipment.project</field>
            <field name="arch" type="xml">
                <tree string="Project">
                    <field name="name"/>
                    <field name="project_code_crm" string="Project Name CRM"/>
                </tree>
            </field>
        </record>

        <record model="ir.ui.view" id="owner_equipment_project_view_form">
            <field name="name">owner.equipment.project.form</field>
            <field name="model">owner.equipment.project</field>
            <field name="arch" type="xml">
                <form string="Project Configuration">
                    <header>
                    </header>
                    <sheet>
                        <group string="Project Configuration">
                            <field name="name"/>
                            <field name="project_code_crm" string="Project Name CRM"/>
                            <field name="project_location_ids" string="Project Locations">
                                <tree>
                                    <field name="name"/>
                                    <field name="location_code"/>
                                </tree>
                            </field>
                            <field name="project_category_ids" string="Equipment Main Categories">
                                <tree>
                                    <field name="name"/>
                                </tree>
                            </field>
                        </group>
                    </sheet>
                </form>
            </field>
        </record>

        <record model="ir.actions.act_window" id="owner_equipment_project_action">
            <field name="name">Owner Equipment Project</field>
            <field name="res_model">owner.equipment.project</field>
            <field name="view_type">form</field>
            <field name="help" type="html">
              <p class="oe_view_nocontent_create">
                Click to add a project.
              </p>
            </field>
        </record>

        <!-- OWNER EQUIPMENT -->
        <record model="ir.ui.view" id="owner_equipment_view_form">
            <field name="name">owner.equipment.form</field>
            <field name="model">owner.equipment</field>
            <field name="arch" type="xml">
                <form string="Equipment Inventory">
                    <header>
                    </header>
                    <sheet>
                        <div class="oe_button_box" name="button_box">
                            <button name="toggle_active" type="object" groups="hr.group_hr_user" class="oe_stat_button" icon="fa-archive">
                                <field name="active" widget="boolean_button" options="{&quot;terminology&quot;: &quot;archive&quot;}"/>
                            </button>
                        </div>
                        <field name="image" widget="image" class="oe_avatar" options="{&quot;preview_image&quot;: &quot;image&quot;, &quot;size&quot;: [200, 200]}"/>
                        <group>
                            <group string="Equipment details">
                                <field name="name"/>
                                <field name="barcode_number" readonly="1"/>
                                <field name="number_of_pieces"/>
                                <field name="weight"/>
                                <field name="total_weight"/>
                                <field name="damaged"/>
                                <field name="category_id" domain="[('project_category_id', '=', category_id)]"/>
                                <field name="subcategory_id" domain="[('owner_subcategory_id', '=', category_id)]"/>
                            </group>
                            <group string="Location details">
                                <field name="project_id" string="Project"/>
                                <field name="removal_responsible_id"/>
                                <field name="date_removed"/>
                                <field name="location_removed_id" domain="[('project_location_id', '=', project_id)]"/>
                                <field name="sub_location_removed_id" domain="[('owner_sublocation_id', '=', location_removed_id)]"/>
                                <field name="stored_container_id"/>
                                <field name="document_link" widget="url" readonly="1"/>
                            </group>
                            <group string="Equipment status">
                                <field name="date_reinstalled" />
                                <field name="location_reinstalled_id" domain="[('project_location_id', '=', project_id)]"/>
                                <field name="sub_location_reinstalled_id" domain="[('owner_sublocation_id', '=', location_reinstalled_id)]"/>
                            </group>
                            <group string="QA/QC Code">
                                <field name="qaqc_item"/>
                                <field name="qaqc_page"/>
                            </group>

                        </group>
                        <notebook>
                            <page string="Description" name="description">
                                <field name="description"/>
                            </page>
                            <page string="Removal Crew" name="removal_crew">
                                <field name="removal_crew_ids" string="Removed By">
                                    <tree>
                                        <field name="employee_crew"/>
                                    </tree>
                                </field>
                            </page>
                            <page string="Equipment tracking" name="equipment_tracking">
                                <field name="equipment_tracking_ids" string="Assigned To">
                                    <tree>
                                        <field name="assigned_employee"/>
                                        <field name="date_assigned_employee"/>
                                        <field name="assigned_location_id" string="Current Location"/>
                                    </tree>
                                </field>
                            </page>
                            <page string="Scan History" name="scan_history">
                                <field name="scan_history_ids" string="Scan History">
                                    <tree>
                                        <field name="equipment_scanned_by"/>
                                        <field name="equipment_scanned_date"/>
                                    </tree>
                                </field>
                            </page>
                        </notebook>
                    </sheet>
                    <div class="oe_chatter">
                        <field name="message_follower_ids" widget="mail_followers"/>
                        <field name="message_ids" widget="mail_thread"/>
                    </div>
                </form>
            </field>
        </record>

        <record model="ir.ui.view" id="owner_equipment_view_kanban">
            <field name="name">owner.equipment.kanban</field>
            <field name="model">owner.equipment</field>
            <field name="arch" type="xml">
                <kanban>
                    <field name="name"/>
                    <field name="barcode_number" />
                    <field name="project_id"/>
                    <field name="location_removed_id"/>
                    <field name="category_id"/>
                    <field name="subcategory_id"/>
                    <field name="removal_responsible_id"/>
                    <field name="date_removed"/>
                    <field name="color"/>
                    <field name="image"/>
                    <templates>
                        <t t-name="kanban-tooltip">
                            <ul class="oe_kanban_tooltip">
                                <li><b>Barcode:</b> <field name="barcode_number"/></li>
                                <li><b>Project:</b> <field name="project_id"/></li>
                            </ul>
                        </t>
                        <t t-name="kanban-box">
                            <div t-attf-class="oe_kanban_color_#{kanban_getcolor(record.color.raw_value)} oe_kanban_card oe_kanban_global_click">
                                <div class="o_dropdown_kanban dropdown">

                                    <a class="dropdown-toggle btn" data-toggle="dropdown" href="#" >
                                        <span class="fa fa-bars fa-lg"/>
                                    </a>
                                    <ul class="dropdown-menu" role="menu" aria-labelledby="dLabel">
                                        <t t-if="widget.editable"><li><a type="edit">Edit...</a></li></t>
                                        <t t-if="widget.deletable"><li><a type="delete">Delete</a></li></t>
                                        <li><ul class="oe_kanban_colorpicker" data-field="color"/></li>
                                    </ul>
                                </div>
                                <div class="oe_kanban_content" tooltip="kanban-tooltip">
                                    <div><b><field name="name"/></b></div>
                                    <div class="mt4">
                                        <div t-if="record.barcode_number.raw_value"><field name="barcode_number"/></div>
                                        <div t-if="record.removal_responsible_id.raw_value"><field name="removal_responsible_id"/></div>
                                        <div t-if="record.location_removed_id.raw_value"><field name="location_removed_id"/></div>
                                        <div t-if="record.date_removed.raw_value"><field name="date_removed"/></div>
                                        <div t-if="record.category_id.raw_value"><field name="category_id"/></div>
                                        <div t-if="record.subcategory_id.raw_value"><field name="subcategory_id"/></div>
                                    </div>
                                    <!--div class="oe_kanban_bottom_right">
                                        <img t-att-src="kanban_image('owner.equipment', 'image', record.image.raw_value)" t-att-title="record.image.value" width="200" height="200" class="oe_kanban_avatar"/>
                                    </div-->
                                </div>
                                <div class="oe_clear"></div>
                            </div>
                        </t>
                    </templates>
                </kanban>
            </field>
        </record>

