<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">
        <record id="menu_vl_subcontractors" model="website.menu">
            <field name="name">Subcontractor</field>
            <field name="url">/vl_subcontractor</field>
            <field name="parent_id" ref="website.main_menu"/>
            <field name="sequence" type="int">50</field>
        </record>
        <record id="action_open_website" model="ir.actions.act_url">
            <field name="name">Website SUbcontractor Form</field>
            <field name="target">self</field>
            <field name="url">/vl_subcontractor</field>
        </record>
        <!-- ne treba record id="base.open_menu" model="ir.actions.todo">
            <field name="action_id" ref="action_open_website"/>
            <field name="state">open</field>
        </record-->
    </data>
    <data>
        <record id="vl_subcontractor.model_vl_subcontractor" model="ir.model">
            <field name="website_form_default_field_id" ref="vl_subcontractor.field_vl_subcontractor_details">
                <!--Modul_where_field_is_first_declared.field_model_fieldname -->
            </field>
            <field name="website_form_access">True</field>
            <field name="website_form_label">Subcontractor Application</field>
        </record>
        <function model="ir.model.fields" name="formbuilder_whitelist">
            <value>vl.subcontractor</value>
            <!--List all editable fields needed in website form  -->
            <value eval="[
                'description',
                'email_from',
                'name',
                'partner_phone'
                'qa_calibration', 'qa_contract_rev', 'campaign_id',
                'qa_sys', 'qa_inspect_test', 'qa_spec_proces',
                'qa_docs', 'qa_non_conf_ite', 'qa_engineering',
                'qa_correct_action', 'qa_purchasing', 'qa_organization',
                'qa_qal_rec', 'qa_handle_stor_pack',
                'qa_acc_inc_stat', 'medium_id',
                'qa_auditors', 'qa_safe_manage', 'source_id',
                'qa_trace_material', 'agent_ship_agent'
            ]"/>
        </function>
    </data>
</odoo>
