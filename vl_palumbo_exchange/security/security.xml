<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record model="ir.module.category" id="module_management">
      <field name="name">Exchange folders</field>
      <field name="description">User access level for this module</field>
      <field name="sequence">3</field>
    </record>

   <record id="group_exchange_user" model="res.groups">
     <field name="name">Exchange User</field>
     <field name="implied_ids" eval="[(4, ref('base.group_user'))]"/>
     <field name="users" eval="[(4, ref('base.user_root'))]"/>
     <field name="category_id" ref="vl_palumbo_exchange.module_management"/>
    </record>

    <record id="group_exchange_administrator" model="res.groups">
      <field name="name">Exchange Administrator</field>
      <field name="implied_ids" eval="[(4, ref('vl_palumbo_exchange.group_exchange_user'))]"/>
      <field name="users" eval="[(4, ref('base.user_root'))]"/>
      <field name="category_id" ref="vl_palumbo_exchange.module_management"/>
    </record>

</odoo>